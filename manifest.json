{
	"update_url": "https://clients2.google.com/service/update2/crx",
	"description": "Let Power Chat CRM be your most powerful assistant for WhatsApp marketing & WhatsApp sales.",
   "version": "2.6.5",
	"manifest_version": 3,
	"name": "Power Chat CRM",
   "short_name": "Power Chat CRM",
	"background": {
		"service_worker": "background.bundle.js"
	},
	"permissions": [
		"tabs",
		"storage",
		"unlimitedStorage",
		"notifications"
	],
	"host_permissions": [
		"*://*.whatsapp.com/*"
	],
	"action": {
		"default_icon": "icon-34.png"
	},
	"chrome_url_overrides": {},
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.whatsapp.com/*"
			],
			"js": [
				"contentScript.bundle.js"
			],
			"css": [
				"content.styles.css"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"icon-128.png",
				"icon-48.png",
				"icon-16.png",
				"icon-34.png",
				"images/*",
				"content.styles.css",
				"inject.styles.css",
				"injectScript.bundle.js",
				"contact_upload.csv",
				"contact_upload.xlsx"
			],
			"matches": [
				"*://*.whatsapp.com/*"
			]
		}
	]
}